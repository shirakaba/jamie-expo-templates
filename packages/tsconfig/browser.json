{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node library",
  "extends": "./base.json",
  "compilerOptions": {
    // See https://aka.ms/tsconfig
    "lib": ["ES2020"],
    "target": "ES2020",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "module": "ES2015",
    "moduleResolution": "Bundler"
  }
}
